{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "start2": "cross-env ENV=testtest nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "testtest": "cross-env ENV=testtest db-migrate --env testtest up && jasmine-ts && cross-env ENV=testtest db-migrate --env testtest down && cross-env ENV=testtest db-migrate --env testtest down && db-migrate --env testtest reset",
    "testtest2": "set ENV=testtest db-migrate --env testtest up && jasmine-ts && cross-env ENV=testtest db-migrate --env testtest down && cross-env ENV=testtest db-migrate --env testtest down && db-migrate --env testtest reset",
    "upupup": "cross-env ENV=testtest db-migrate --env testtest up",
    "downdowndown": "db-migrate --env testtest reset",
    "upup": "cross-env ENV=testtest db-migrate --env testtest up",
    "downdown_migrate": "cross-env ENV=testtest db-migrate --env testtest down",
    "downdown": "db-migrate db:drop testtest",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.14",
    "pg": "^8.7.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.10.2",
    "cross-env": "^7.0.3",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
